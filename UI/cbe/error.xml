<?xml version="1.0" encoding="utf-8"?>
<view xmlns:l="http://xios3.com/lang" name="CommunicationError" title="Communication Error" author="Daniel Arthursson" created="2004-08-06" revised="2004-08-06" icon="icon://applications/error" height="380" width="400" l:title="Communication Error">
  <panel type="row" height="100%" padding="5">
    <panel padding="10" look="outset">
      <panel type="form" cols="2" label-align="left" width="100%" label-pos="left">
        <row>
          <image style="margin-left: 2px;" align="left" valign="middle" src="icons/network/32x32/client_network.png"/>
          <label style="text-align: right; margin-right: 12px;" name="Description" font="verdana" color="#ffffff" size="17" default="Communication Channel Error" l:default="Communication Channel Error"/>
        </row>
        <line/>
        <row>
          <label name="Channel" label="Channel" l:label="Channel" default="N/A"/>
        </row>
        <row>
          <label name="Service" label="Service" l:label="Service" default="N/A"/>
        </row>
        <row>
          <label name="Accessed" label="Accessed" l:label="Accessed" default="N/A"/>
        </row>
        <line/>
        <row>
          <label name="Method" label="Method" l:label="Method" default="N/A"/>
        </row>
        <row>
          <textarea name="Response" label="Response" labelPos="topleft" width="100%" mode="xml" l:label="Response"/>
        </row>
        <line/>
        <row>
          <label name="Code" label="Error Code" l:label="Error Code" default="0"/>
        </row>
      </panel>
    </panel>
    <panel type="flow" align="right">
      <button name="OK" text="OK" width="80" l:text="OK"/>
    </panel>
  </panel>
</view>
